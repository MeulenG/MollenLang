behavior Division {
    action divide(int a, int b) {
        if (b != 0) {
            let result = a / b;
            Console.print(a + " / " + b + " = " + result);
            return result;
        } else {
            Console.print("Error: Division by zero");
            return null;
        }
    }
}

object div inherits Division

div.divide(8, 2);
div.div(5, 0);