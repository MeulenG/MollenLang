behavior Division {
    action divide(def int a, def int b) {
        if (b != 0) {
            def int result = a / b;
            print(a + " / " + b + " = " + result);
            return result;
        } else {
            print("Error: Division by zero");
            return null;
        }
    }
}

object div inherits Division

div.divide(8, 2);